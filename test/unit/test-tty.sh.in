#!@SH_PROG@
# -*- shell-script -*-
PS4='(%x:%I): [%?] zsh+ 
'
setopt ksharrays

test_tty()
{
    _Dbg_is_tty $0
    assertEquals "1" "$?"
    tty=$(tty)
    if (($? == 0)); then 
	if [[ $tty =~ 'not a tty' ]] ; then
	    result=1
	    :
	else
	    result=0
	fi
	_Dbg_is_tty $tty
	assertEquals "$result" "$?"
	if ((result == 0)) ; then
	    _Dbg_set_tty $tty
	fi
    fi
}


if [ '@abs_top_srcdir@' = '' ] ; then
  echo "Something is wrong abs_top_srcdir is not set."
 exit 1
fi
abs_top_srcdir=@abs_top_srcdir@
# Make sure @abs_top_srcr@ has a trailing slash
abs_top_srcdir=${abs_top_srcdir%%/}/

. $abs_top_srcdir/lib/tty.sh

set -o shwordsplit
SHUNIT_PARENT=$0

# load shunit2
shunit_file=${abs_top_srcdir}test/unit/shunit2
. ${shunit_file}
